@article{cheuk_mathrmlambda-enhanced_2018,
  title = {$\mathrm{\Lambda}$-Enhanced Imaging of Molecules in an Optical Trap},
  author = {Cheuk, Lawrence W. and Anderegg, LoÃ¯c and Augenbraun, Benjamin L. and Bao, Yicheng and Burchesky, Sean and Ketterle, Wolfgang and Doyle, John M.},
  year = {2018},
