@article{giovanazzi_tuning_2002,
  title = {Tuning the {{Dipolar Interaction}} in {{Quantum Gases}}},
  author = {Giovanazzi, Stefano and G{\"o}rlitz, Axel and Pfau, Tilman},
  year = {2002},
  month = sep,
  volume = {89},
  pages = {130401},
  publisher = {{American Physical Society}},
  doi = {10.1103/PhysRevLett.89.130401},
  abstract = {We have studied the tunability of the interaction between permanent dipoles in Bose-Einstein condensates. Based on time-dependent control of the anisotropy of the dipolar interaction, we show that even the very weak magnetic dipole coupling in alkali gases can be used to excite collective modes. Furthermore, we discuss how the effective dipolar coupling in a Bose-Einstein condensate can be tuned from positive to negative values and even switched off completely by fast rotation of the orientation of the dipoles.},
  journal = {Physical Review Letters},
  keywords = {Dipolar Interactions,Quantum Gases},
  number = {13}
