@article{wynar_molecules_2000,
  title = {Molecules in a {{Bose}}-{{Einstein Condensate}}},
  author = {Wynar, Roahn and Freeland, R. S. and Han, D. J. and Ryu, C. and Heinzen, D. J.},
  year = {2000},
  month = feb,
  volume = {287},
