@article{dunham_energy_1932,
  title = {The {{Energy Levels}} of a {{Rotating Vibrator}}},
  author = {Dunham, J. L.},
  year = {1932},
  month = sep,
  volume = {41},
  pages = {721--731},
  publisher = {{American Physical Society}},
  doi = {10.1103/PhysRev.41.721},
  abstract = {The energy levels of a rotating vibrator are calculated in considerable detail by means of the Wentzel-Brillouin-Kramers method. The new terms determined are {$\omega$}ez and a set of correction terms which appear in the earlier members of the equation. These correction terms enter in such a way that {$\omega$}e is not exactly the coefficient of (v+12); Be is not exactly the coefficient of K(K+1), etc. However the differences are small and are detectable only in the case of light molecules. The correction terms are of the magnitude of Be2{$\omega$}e2. Formulas for the effect of the correction terms on isotope shifts are given, and for the calculation of the correction terms themselves. Also a method is given for obtaining actual potential functions from band spectrum data, based on Morse's potential function. Finally the numerical magnitude of the correction terms for several states of H2 and for NaH is discussed.},
  journal = {Physical Review},
  keywords = {Spectroscopy},
  number = {6}
}

@article{eckel_search_2013,
  title = {Search for the Electron Electric Dipole Moment Using \$\textbackslash ensuremath\{\textbackslash\vphantom\}{{Omega}}\vphantom\{\}\$-Doublet Levels in {{PbO}}},
  author = {Eckel, S. and Hamilton, P. and Kirilov, E. and Smith, H. W. and DeMille, D.},
  year = {2013},
  month = may,
  volume = {87},
  pages = {052130},
  publisher = {{American Physical Society}},
  issn = {1050-2947},
  doi = {10.1103/PhysRevA.87.052130},
  abstract = {We present results of an experiment to probe for the electric dipole moment (EDM) of the electron using an {$\Omega$}-doublet state in a polar molecule. If the molecule is both massive and has a large molecular-fixed frame dipole moment, then the {$\Omega$}-doublet states have the potential to greatly increase the sensitivity of experiments searching for the EDM while also allowing for new methods of systematic error rejection. Here, we use the metastable a(1)3{$\Sigma$}+ state of lead monoxide (PbO) to probe for the electron EDM. Our best fit for the electron EDM of de=(-4.4{$\pm$}9.5stat{$\pm$}1.8syst)\texttimes 10-27ecm allows us to place an upper limit on the magnitude of the EDM of {$\mid\mid$}de|{$<$}1.7\texttimes 10-26ecm (90\% confidence). While this is less stringent than limits from other, previous experiments, our work emphasizes the systematic error rejection properties associated with the {$\Omega$}-doublet level structure. The results should inform the work of other, ongoing experiments that use molecules with analogous level structure.},
  journal = {Physical Review A},
  keywords = {EDM Experiments},
  number = {5}
}

