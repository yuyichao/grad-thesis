@article{blume_fermi_2002,
  title = {Fermi Pseudopotential Approximation: {{Two}} Particles under External Confinement},
  shorttitle = {Fermi Pseudopotential Approximation},
  author = {Blume, D. and Greene, Chris H.},
  year = {2002},
  month = apr,
  volume = {65},
  pages = {043613},
  publisher = {{American Physical Society}},
  doi = {10.1103/PhysRevA.65.043613},
  abstract = {In this paper we calculate the energy levels of two trapped atoms interacting through a spherical two-body potential V(r) under external confinement quantum mechanically. We then replace V(r) by either a regularized energy-independent or an energy-dependent {$\delta$}-function potential. A comparison shows that the use of an energy-dependent pseudopotential improves significantly upon the use of an energy-independent pseudopotential. The accuracy of the pseudopotential approximation is approximately determined by the ratio {$\beta$}6/aho, where {$\beta$}6 and aho denote the characteristic length scale of the two-body interaction potential V(r) and of the trapping potential, respectively.},
  journal = {Physical Review A},
  number = {4}
}

