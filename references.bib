@article{zhang_forming_2020,
  title = {Forming a {{Single Molecule}} by {{Magnetoassociation}} in an {{Optical Tweezer}}},
  author = {Zhang, Jessie T. and Yu, Yichao and Cairncross, William B. and Wang, Kenneth and Picard, Lewis R. B. and Hood, Jonathan D. and Lin, Yen-Wei and Hutson, Jeremy M. and Ni, Kang-Kuen},
  year = {2020},
