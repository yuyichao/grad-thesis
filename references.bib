@article{bickman_preparation_2009,
  title = {Preparation and Detection of States with Simultaneous Spin Alignment and Selectable Molecular Orientation in {{PbO}}},
  author = {Bickman, S. and Hamilton, P. and Jiang, Y. and DeMille, D.},
  year = {2009},
  month = aug,
  volume = {80},
  pages = {023418},
  publisher = {{American Physical Society}},
  issn = {1050-2947},
  doi = {10.1103/PhysRevA.80.023418},
  abstract = {We are pursuing an experiment to measure the electric dipole moment of the electron using the molecule PbO. This measurement requires the ability to prepare quantum states with selectable orientation of the molecular axis and, simultaneously, alignment of the electron spin perpendicular to this axis. It also requires efficient detection of the evolution of the spin-alignment direction within such a state. We describe a series of experiments that have achieved these goals, and the features and limitations of the techniques. Our results include observation of and explanation for a small electric-field dependence of the magnetic g factor in the oriented states. We also discuss possible approaches for improved efficiency in this and similar systems.},
  journal = {Physical Review A},
  keywords = {EDM Experiments,Other EDM Experiments},
  number = {2}
}

@article{bloch_many-body_2008,
