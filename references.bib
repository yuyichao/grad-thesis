@article{cairncross_precision_2017,
  title = {Precision {{Measurement}} of the {{Electron}}'s {{Electric Dipole Moment Using Trapped Molecular Ions}}},
  author = {Cairncross, William B. and Gresh, Daniel N. and Grau, Matt and Cossel, Kevin C. and Roussy, Tanya S. and Ni, Yiqi and Zhou, Yan and Ye, Jun and Cornell, Eric A.},
  year = {2017},
  month = oct,
  volume = {119},
  pages = {153001},
  publisher = {{American Physical Society}},
  doi = {10.1103/PhysRevLett.119.153001},
  abstract = {We describe the first precision measurement of the electron's electric dipole moment (de) using trapped molecular ions, demonstrating the application of spin interrogation times over 700 ms to achieve high sensitivity and stringent rejection of systematic errors. Through electron spin resonance spectroscopy on 180Hf19F+ in its metastable 3{$\Delta$}1 electronic state, we obtain de=(0.9{$\pm$}7.7stat{$\pm$}1.7syst)\texttimes 10-29 e cm, resulting in an upper bound of |de|{$<$}1.3\texttimes 10-28 e cm (90\% confidence). Our result provides independent confirmation of the current upper bound of |de|{$<$}9.4\texttimes 10-29 e cm [J. Baron et al., New J. Phys. 19, 073029 (2017)], and offers the potential to improve on this limit in the near future.},
  journal = {Physical Review Letters},
  keywords = {EDM Experiments},
  number = {15}
}

@article{cheuk_mathrmlambda-enhanced_2018,
  title = {$\mathrm{\Lambda}$-Enhanced Imaging of Molecules in an Optical Trap},
  author = {Cheuk, Lawrence W. and Anderegg, Loïc and Augenbraun, Benjamin L. and Bao, Yicheng and Burchesky, Sean and Ketterle, Wolfgang and Doyle, John M.},
  year = {2018},
  month = aug,
  volume = {121},
  pages = {083201},
  publisher = {American Physical Society},
  doi = {10.1103/PhysRevLett.121.083201},
  abstract = {We report on nondestructive imaging of optically trapped calcium monofluoride molecules using in situ $\mathrm{\Lambda}$-enhanced gray molasses cooling. 200~times more fluorescence is obtained compared to destructive on-resonance imaging, and the trapped molecules remain at a temperature of $20~\mathrm{\mu K}$. The achieved number of scattered photons makes possible nondestructive single-shot detection of single molecules with high fidelity.},
  journal = {Physical Review Letters},
  number = {8}
}

@article{colzi_sub-doppler_2016,
  title = {Sub-{{Doppler}} Cooling of Sodium Atoms in Gray Molasses},
  author = {Colzi, Giacomo and Durastante, Gianmaria and Fava, Eleonora and Serafini, Simone and Lamporesi, Giacomo and Ferrari, Gabriele},
  year = {2016},
  volume = {93},
  pages = {1--6},
  issn = {10941622},
  doi = {10.1103/PhysRevA.93.023421},
  abstract = {We report on the realization of sub-Doppler laser cooling of sodium atoms in gray molasses using the D1 optical transition (3s 2 S 1/2 \textrightarrow{} 3p 2 P 1/2) at 589.8 nm. The technique is applied to samples containing 3 \texttimes{} 10 9 atoms, previously cooled to 350 \textmu K in a magneto-optical trap, and it leads to temperatures as low as 9 \textmu K and phase-space densities in the range of 10 -4 . The capture efficiency of the gray molasses is larger than 2/3, and we observe no density-dependent heating for densities up to 10 11 cm -3 .},
  journal = {Physical Review A},
  number = {2}
}

