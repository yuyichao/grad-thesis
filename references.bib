@article{stellmer_creation_2012,
  title = {Creation of Ultracold ${\mathrm{Sr}}_{2}$ Molecules in the Electronic Ground State},
  author = {Stellmer, Simon and Pasquiou, Benjamin and Grimm, Rudolf and Schreck, Florian},
  year = {2012},
  month = sep,
  volume = {109},
  pages = {115302},
  publisher = {American Physical Society},
  doi = {10.1103/PhysRevLett.109.115302},
  abstract = {We report on the creation of ultracold $^{84}{\mathrm{Sr}}_{2}$ molecules in the electronic ground state. The molecules are formed from atom pairs on sites of an optical lattice using stimulated Raman adiabatic passage (STIRAP). We achieve a transfer efficiency of $30~\mathrm{\%}$ and obtain $4\times10^4$ molecules with full control over the external and internal quantum state. STIRAP is performed near the narrow $\mathrm{^1S_0-^3P_1}$ intercombination transition, using a vibrational level of the $1(0^+_u)$ potential as an intermediate state. In preparation of our molecule association scheme, we have determined the binding energies of the last vibrational levels of the $1(0^+_u)$, $1(1_u)$ excited-state and the $\mathrm{X^1\Sigma^+_g}$ ground-state potentials. Our work overcomes the previous limitation of STIRAP schemes to systems with magnetic Feshbach resonances, thereby establishing a route that is applicable to many systems beyond alkali-metal dimers.},
  journal = {Physical Review Letters},
  keywords = {Optical Tweezers,STIRAP},
  number = {11}
}

