@article{yu_motional-ground-state_2018,
	title = {Motional-ground-state cooling outside the Lamb-Dicke regime},
	volume = {97},
	rights = {All rights reserved},
	issn = {2469-9926, 2469-9934},
	url = {https://link.aps.org/doi/10.1103/PhysRevA.97.063423},
	doi = {10.1103/PhysRevA.97.063423},
	abstract = {We report Raman sideband cooling of a single sodium atom to its three-dimensional motional ground state in an optical tweezer. Despite a large Lamb-Dicke parameter, high initial temperature, and large differential light shifts between the excited state and the ground state, we achieve a ground state population of \$93.5(7)\$\% after \$53\$ ms of cooling. Our technique includes addressing high-order sidebands, where several motional quanta are removed by a single laser pulse, and fast modulation of the optical tweezer intensity. We demonstrate that Raman sideband cooling to the 3D motional ground state is possible, even without tight confinement and low initial temperature.},
	pages = {063423},
	number = {6},
	journaltitle = {Physical Review A},
	shortjournal = {Phys. Rev. A},
	author = {Yu, Y. and Hutzler, N. R. and Zhang, J. T. and Liu, L. R. and Hood, J. D. and Rosenband, T. and Ni, K.-K.},
	urldate = {2019-12-09},
	date = {2018-06-28},
	langid = {english},
	eprinttype = {arxiv},
	eprint = {1708.03296},
	keywords = {Quantum Gases, Atomic Physics, Chemical Physics},
	file = {Yu et al. - 2018 - Motional-ground-state cooling outside the Lamb-Dic.pdf:files/1811/Yu et al. - 2018 - Motional-ground-state cooling outside the Lamb-Dic.pdf:application/pdf}
}

@article{liu_molecular_2019,
	title = {Molecular assembly of ground state cooled single atoms},
	volume = {9},
	rights = {All rights reserved},
	issn = {2160-3308},
	url = {http://arxiv.org/abs/1902.03935},
	doi = {10.1103/PhysRevX.9.021039},
	abstract = {We demonstrate full quantum state control of two species of single atoms using optical tweezers and assemble the atoms into a molecule. Our demonstration includes 3D ground-state cooling of a single atom (Cs) in an optical tweezer, transport by several microns with minimal heating, and merging with a single Na atom. Subsequently, both atoms occupy the simultaneous motional ground state with 61(4){\textbackslash}\% probability. This realizes a sample of exactly two co-trapped atoms near the phase-space-density limit of one, and allows for efficient stimulated-Raman transfer of a pair of atoms into a molecular bound state of the triplet electronic ground potential \$a{\textasciicircum}3{\textbackslash}Sigma{\textasciicircum}+\$. The results are key steps toward coherent creation of single ultracold molecules, for future exploration of quantum simulation and quantum information processing.},
	pages = {021039},
	number = {2},
	journaltitle = {Physical Review X},
	shortjournal = {Phys. Rev. X},
	author = {Liu, Lee R. and Hood, Jonathan D. and Yu, Yichao and Zhang, Jessie T. and Wang, Kenneth and Lin, Yen-Wei and Rosenband, Till and Ni, Kang-Kuen},
	urldate = {2019-12-15},
	date = {2019-05-24},
	langid = {english},
	eprinttype = {arxiv},
	eprint = {1902.03935},
	keywords = {Ultracold Molecules, Ultracold Atoms, Optical Tweezers},
	file = {Liu et al. - 2019 - Molecular assembly of ground state cooled single a.pdf:files/2610/Liu et al. - 2019 - Molecular assembly of ground state cooled single a.pdf:application/pdf}
}

