	eprinttype = {arxiv},
	eprint = {2003.07850},
	keywords = {Condensed Matter - Quantum Gases, Physics - Atomic Physics, Physics - Chemical Physics},
	file = {APS Snapshot:files/4156/PhysRevLett.124.html:text/html;arXiv.org Snapshot:files/3429/2003.html:text/html;Zhang et al_2020_Forming a Single Molecule by Magnetoassociation in an Optical Tweezer.pdf:files/4155/Zhang et al_2020_Forming a Single Molecule by Magnetoassociation in an Optical Tweezer.pdf:application/pdf}
